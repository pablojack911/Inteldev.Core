<Grid x:Class="Inteldev.Core.Presentacion.Controles.ItemFormularioCheckAjustable"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="26" Height="25">
    
    <CheckBox Content="{Binding Etiqueta, RelativeSource={RelativeSource AncestorType={x:Type Grid}}}"
        IsChecked="{Binding Seleccionado, Mode=TwoWay,RelativeSource={RelativeSource AncestorType={x:Type Grid}} }"
        Name="checkBox"
        VerticalAlignment="Center"
        Margin="10,0,10,0"
            
    />
    
</Grid>

